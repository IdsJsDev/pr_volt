{
    "env": {
        "browser": true,
        "es2021": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": [
        "@wemake-services/typescript/recommended"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
//        "react/jsx-uses-react": "off",
//        "react/react-in-jsx-scope": "off",
//        "@typescript-eslint/no-unused-vars": ["off"],
        "@typescript-eslint/no-extra-parens": "off"
    }
}
